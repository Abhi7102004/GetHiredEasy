{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://get-hired-easy.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
